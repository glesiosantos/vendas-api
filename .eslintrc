{
  "extends": "standard-with-typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-inline-comments": "warn",
    "multiline-comment-style": "warn",
    "spaced-comment": "warn",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/restrict-template-expressions": "off"
  }
}
